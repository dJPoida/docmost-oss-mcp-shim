# Public URL of your Docmost instance (behind NGINX/Let's Encrypt)
DOCMOST_BASE_URL=

# Docmost account the shim will use to log in
DOCMOST_EMAIL=
DOCMOST_PASSWORD=

# Shim bind (keep localhost unless you proxy it)
HOST=127.0.0.1
PORT=3888

# Optional: require clients to include X-SHIM-KEY header
SHIM_API_KEY=change-this-long-random-string